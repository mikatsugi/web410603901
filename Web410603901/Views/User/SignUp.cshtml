@model Web410603901.ViewModels.SignUpData

@{
    ViewBag.Title = "SignUp";
}

<h2>SignUp</h2>

@using (Html.BeginForm("SignUp", "User", FormMethod.Post))
{

    <p>姓名:@Html.TextBoxFor( m => m.Name) @Model.NameMessage</p>
    <p>帳號:@Html.TextBoxFor( m => m.Account) @Model.AccountMessage</p>
    <p>密碼:@Html.PasswordFor( m => m.Password) @Model.PasswordMessage</p>

    <p>@Model.Message</p>

    <input type="submit" value="註冊" />
}



