
@{
    ViewBag.Title = "HW2";
}

<p>印出1-10</p>
@for (int i = 1; i <= 10; i++)
{
    <p>@i</p>
}

<h2>HW2 @DateTime.Now</h2>

<p>姓名: @Html.TextBox("Name")</p>
<p>帳號: @Html.TextBox("Account")</p>
<p>密碼: @Html.TextBox("Password")</p>
<p>
    性別:
    @Html.RadioButton("gender", "A") 男生
    @Html.RadioButton("gender", "B") 女生
</p>
<p>
    興趣:
    @Html.CheckBox("Reading", "R") 閱讀
    @Html.CheckBox("Reading", "R") 游泳
</p>
<p>
    自我介紹:
    @Html.TextArea("About")
</p>

